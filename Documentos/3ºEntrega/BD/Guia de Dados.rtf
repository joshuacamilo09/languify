\fs24 \cf0 Mensagens \'f3rf\'e3s:
\f1\b0 \
\pard\pardeftab720\partightenfactor0

\f2\fs26 \cf0 -- Verificar mensagens sem conversa\
SELECT cm.id, cm.conversation_id\
FROM conversation_messages cm\
LEFT JOIN conversations c ON cm.conversation_id = c.id\
WHERE c.id IS NULL;\
\pard\pardeftab720\sa240\partightenfactor0

\f0\b\fs24 \cf0 Conversas sem mensagens:
\f1\b0 \
\pard\pardeftab720\partightenfactor0

\f2\fs26 \cf0 -- Listar conversas vazias\
SELECT c.id, c.title, c.state\
FROM conversations c\
LEFT JOIN conversation_messages cm ON c.id = cm.conversation_id\
WHERE cm.id IS NULL;\
\pard\pardeftab720\sa240\partightenfactor0

\f0\b\fs24 \cf0 Conversas presas em INITIALIZING:
\f1\b0 \
\pard\pardeftab720\partightenfactor0

\f2\fs26 \cf0 -- Conversas em inicializa\'e7\'e3o h\'e1 mais de 5 minutos\
SELECT id, title, created_at\
FROM conversations\
WHERE state = 'INITIALIZING'\
AND created_at < NOW() - INTERVAL '5 minutes';\
